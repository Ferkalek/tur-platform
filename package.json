{
  "name": "tur-platform",
  "version": "1.0.0",
  "description": "Angular frontend and NestJS backend application",
  "homepage": "https://github.com/Ferkalek/tur-platform#readme",
  "bugs": {
    "url": "https://github.com/Ferkalek/tur-platform/issues"
